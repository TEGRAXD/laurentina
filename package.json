{
  "name": "laurentina",
  "version": "1.0.5",
  "description": "Audio controller and queue wrapper for Shoukaku and Discord.js to manage music playback.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "rollup -c",
    "watch": "npx rollup --config --watch",
    "test": "jest"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "discord-api-types": "^0.37.97",
    "discord.js": "^14.15.3",
    "shoukaku": "^4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "rollup": "^4.18.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.2"
  },
  "author": "Tegar B. Suganda",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TEGRAXD/laurentina.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "keywords": [
    "laurentina",
    "discord",
    "shoukaku",
    "queue",
    "wrapper",
    "music"
  ]
}
